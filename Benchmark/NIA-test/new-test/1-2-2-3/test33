(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* 7 (pow x1)) (* 78465 x1) (* (- 62228) x2) (* (- 98103) x3)) 63542))
(assert (<= (+ 0 (* (- 99) (pow x1)) (* 55080 x1) (* (- 95077) x2) (* 69415 x3))
    (- 39531)))
(assert (<= (+ 0 (* (- 36782) x1) (* 97443 x2) (* 62226 x3)) 48728))
(assert (<= (+ 0 (* (- 82819) x1) (* 76038 x2) (* 67335 x3)) (- 31139)))
(assert (<= (+ 0 (* (- 92993) x1) (* (- 24749) x2) (* (- 50912) x3)) 34129))
(check-sat)
(get-model)
