(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (= (+ 0 (* (- 100) (pow x1)) (* (- 91507) x1) (* (- 44007) x2) (* 58412 x3))
   (- 19078)))
(assert (<= (+ 0 (* 97 (pow x1)) (* 44443 x1) (* 36925 x2) (* 27588 x3)) (- 98885)))
(assert (<= (+ 0 (* 14368 x1) (* (- 71654) x2) (* 62664 x3)) 95450))
(assert (<= (+ 0 (* 51359 x1) (* 94281 x2) (* (- 64639) x3)) (- 56853)))
(assert (<= (+ 0 (* (- 39481) x1) (* 98507 x2) (* (- 25587) x3)) 35108))
(check-sat)
(get-model)
