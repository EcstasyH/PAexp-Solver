(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 6) (pow x1)) (* (- 27100) x1) (* (- 25417) x2) (* 72524 x3))
    (- 45688)))
(assert (<= (+ 0 (* 98 (pow x1)) (* 5928 x1) (* (- 6018) x2) (* (- 7444) x3)) 30399))
(assert (<= (+ 0 (* (- 84217) x1) (* 98530 x2) (* 86049 x3)) 2256))
(assert (<= (+ 0 (* (- 98263) x1) (* (- 21627) x2) (* 72355 x3)) 20156))
(assert (<= (+ 0 (* 15924 x1) (* (- 28068) x2) (* (- 41170) x3)) 51980))
