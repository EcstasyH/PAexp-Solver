(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 28) (pow x1)) (* (- 30167) x1) (* 87351 x2) (* (- 47781) x3))
    (- 6606)))
(assert (<= (+ 0 (* 69 (pow x1)) (* (- 7697) x1) (* 85703 x2) (* (- 49763) x3))
    (- 14282)))
(assert (<= (+ 0 (* 40017 x1) (* (- 19995) x2) (* 58030 x3)) 82223))
(assert (<= (+ 0 (* (- 70203) x1) (* 81136 x2) (* 46710 x3)) (- 47984)))
(assert (<= (+ 0 (* (- 10352) x1) (* 1158 x2) (* (- 80705) x3)) 23446))
