    (- 23322)))
(assert (<= (+ 0 (* 8 (pow x1)) (* 16868 x1) (* (- 7348) x2) (* 13242 x3)) (- 53602)))
(assert (<= (+ 0 (* 6 x1) (* 90 x2) (* (- 6) x3)) (- 4752)))
(assert (<= (+ 0 (* (- 30) x1) (* 59 x2) (* (- 66) x3)) 36939))
(assert (<= (+ 0 (* 42 x1) (* 44 x2) (* (- 61) x3)) 20388))
