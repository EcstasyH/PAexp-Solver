(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 10) (pow x1)) (* 21593 x1) (* 92715 x2) (* (- 55628) x3)) 19984))
(assert (<= (+ 0 (* 4 (pow x1)) (* 17422 x1) (* 2022 x2) (* 85089 x3)) (- 26133)))
(assert (<= (+ 0 (* 44 x1) (* 87 x2) (* 86 x3)) (- 94648)))
(assert (<= (+ 0 (* 10 x1) (* (- 68) x2) (* 54 x3)) (- 34000)))
(assert (<= (+ 0 (* 17 x1) (* (- 70) x2) (* 98 x3)) (- 72506)))
(check-sat)
(get-model)
