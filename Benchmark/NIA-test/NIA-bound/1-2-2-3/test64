(assert (<= (+ 0 (* (- 6) (pow x1)) (* (- 43851) x1) (* 81946 x2) (* 32663 x3)) 7890))
(assert (<= (+ 0 (* 100 x1) (* 22 x2) (* (- 53) x3)) (- 7861)))
(assert (<= (+ 0 (* 13 x1) (* (- 65) x2) (* (- 55) x3)) 32718))
(assert (<= (+ 0 (* (- 17) x1) (* 56 x2) (* 25 x3)) 47559))
