(assert (<= (+ 0 (* (- 3) (pow x1)) (* (- 60212) x1) (* 89321 x2) (* (- 61590) x3))
    (- 32791)))
(assert (<= (+ 0 (* 5 x1) (* 65 x2) (* 57 x3)) 37119))
(assert (<= (+ 0 (* 51 x1) (* (- 15) x2) (* (- 8) x3)) (- 63094)))
