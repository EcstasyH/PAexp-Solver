(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 10) (pow x1)) (* 3532 x1) (* 97321 x2) (* (- 94188) x3)) 80812))
(assert (<= (+ 0 (* 4 (pow x1)) (* (- 56357) x1) (* 37521 x2) (* (- 99901) x3)) 80331))
(assert (<= (+ 0 (* (- 18) x1) (* 82 x2) (* 57 x3)) 67283))
(assert (<= (+ 0 (* (- 33) x1) (* 72 x2) (* (- 75) x3)) 72384))
(assert (<= (+ 0 (* 81 x1) (* (- 66) x2) (* 24 x3)) (- 39467)))
