(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 6) (pow x1)) (* 45841 x1) (* (- 20229) x2) (* 68312 x3))
    (- 34476)))
(assert (<= (+ 0 (* (- 2) (pow x1)) (* (- 99887) x1) (* 16506 x2) (* (- 14149) x3))
    (- 84181)))
(assert (<= (+ 0 (* (- 98) x1) (* (- 80) x2) (* (- 82) x3)) (- 48064)))
(assert (<= (+ 0 (* 87 x1) (* (- 81) x2) (* 9 x3)) 99506))
(assert (<= (+ 0 (* 8 x1) (* 39 x2) (* (- 1) x3)) 38007))
