(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* 10 (pow x1)) (* 80574 x1) (* (- 61466) x2) (* (- 68231) x3)) 71765))
(assert (<= (+ 0 (* 1 (pow x1)) (* 73848 x1) (* (- 31358) x2) (* (- 49489) x3))
    (- 27272)))
(assert (<= (+ 0 (* (- 39) x1) (* 93 x2) (* 47 x3)) 3580))
(assert (<= (+ 0 (* 46 x1) (* 59 x2) (* 89 x3)) 3141))
(assert (<= (+ 0 (* 84 x1) (* (- 86) x2) (* 0 x3)) (- 23454)))
(check-sat)
(get-model)
