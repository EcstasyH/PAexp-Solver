(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 10) (pow x1)) (* 72771 x1) (* (- 8999) x2) (* 43742 x3)) 20330))
(assert (<= (+ 0 (* 2 (pow x1)) (* (- 84207) x1) (* (- 17378) x2) (* (- 75281) x3))
    (- 12079)))
(assert (<= (+ 0 (* (- 14) x1) (* 79 x2) (* (- 45) x3)) (- 52269)))
(assert (<= (+ 0 (* 47 x1) (* 45 x2) (* (- 85) x3)) 11896))
(assert (<= (+ 0 (* 55 x1) (* 23 x2) (* 93 x3)) 52336))
(check-sat)
(get-model)
