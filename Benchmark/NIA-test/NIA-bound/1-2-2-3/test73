(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 8) (pow x1)) (* 81816 x1) (* 8296 x2) (* (- 69943) x3)) 36053))
(assert (<= (+ 0 (* 4 (pow x1)) (* (- 45829) x1) (* 96780 x2) (* 64621 x3)) (- 45094)))
(assert (<= (+ 0 (* 59 x1) (* 66 x2) (* 62 x3)) 17333))
(assert (<= (+ 0 (* (- 58) x1) (* 78 x2) (* 74 x3)) 47981))
(assert (<= (+ 0 (* 17 x1) (* (- 70) x2) (* 23 x3)) (- 67403)))
(check-sat)
