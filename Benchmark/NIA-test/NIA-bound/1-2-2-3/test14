(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 3) (pow x1)) (* 98596 x1) (* 49175 x2) (* (- 81332) x3))
    (- 17741)))
(assert (<= (+ 0 (* 1 (pow x1)) (* (- 92393) x1) (* 9089 x2) (* (- 54816) x3)) 27784))
(assert (<= (+ 0 (* 74 x1) (* (- 66) x2) (* 72 x3)) 3997))
(assert (<= (+ 0 (* (- 75) x1) (* (- 1) x2) (* 60 x3)) 49992))
(assert (<= (+ 0 (* 37 x1) (* (- 22) x2) (* (- 25) x3)) (- 59222)))
(check-sat)
(get-model)
