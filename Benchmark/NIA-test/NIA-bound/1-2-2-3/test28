(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* 3 (pow x1)) (* 796 x1) (* (- 98284) x2) (* (- 18067) x3)) (- 16181)))
(assert (<= (+ 0 (* (- 4) (pow x1)) (* (- 92130) x1) (* (- 98785) x2) (* 408 x3))
    (- 61600)))
(assert (<= (+ 0 (* 100 x1) (* (- 35) x2) (* (- 70) x3)) (- 33840)))
(assert (<= (+ 0 (* 43 x1) (* 48 x2) (* (- 23) x3)) 62617))
(assert (<= (+ 0 (* (- 59) x1) (* 57 x2) (* 42 x3)) (- 91)))
(check-sat)
(get-model)
