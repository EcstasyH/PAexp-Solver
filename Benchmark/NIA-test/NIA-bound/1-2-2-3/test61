(assert (<= (+ 0 (* 6 (pow x1)) (* 6411 x1) (* 25470 x2) (* (- 84498) x3)) 35801))
(assert (<= (+ 0 (* (- 1) (pow x1)) (* 87302 x1) (* (- 6195) x2) (* 24980 x3)) 47858))
(assert (<= (+ 0 (* (- 30) x1) (* (- 44) x2) (* (- 21) x3)) 34465))
