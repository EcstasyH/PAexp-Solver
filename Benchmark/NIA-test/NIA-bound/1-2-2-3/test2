(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 5) (pow x1)) (* 10508 x1) (* 7833 x2) (* (- 17208) x3))
    (- 18177)))
(assert (<= (+ 0 (* (- 10) (pow x1)) (* (- 14137) x1) (* (- 67858) x2) (* 3397 x3))
    (- 15147)))
(assert (<= (+ 0 (* 62 x1) (* (- 35) x2) (* (- 77) x3)) 21238))
(assert (<= (+ 0 (* (- 49) x1) (* 23 x2) (* (- 26) x3)) (- 71705)))
(assert (<= (+ 0 (* 91 x1) (* 41 x2) (* 15 x3)) 82710))
(check-sat)
(get-model)
