(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 8) (pow x1)) (* 61039 x1) (* 98677 x2) (* (- 89406) x3))
    (- 73565)))
(assert (<= (+ 0 (* 3 (pow x1)) (* (- 29863) x1) (* 64248 x2) (* (- 57569) x3)) 41462))
(assert (<= (+ 0 (* 30 x1) (* (- 84) x2) (* (- 67) x3)) 99981))
(assert (<= (+ 0 (* 10 x1) (* (- 6) x2) (* (- 61) x3)) (- 8029)))
(assert (<= (+ 0 (* (- 75) x1) (* 79 x2) (* 94 x3)) (- 18452)))
(check-sat)
(get-model)
