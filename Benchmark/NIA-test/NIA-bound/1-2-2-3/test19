(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 8) (pow x1)) (* (- 99135) x1) (* 97387 x2) (* 98130 x3))
    (- 17513)))
(assert (<= (+ 0 (* 6 (pow x1)) (* (- 6258) x1) (* (- 55325) x2) (* (- 68257) x3))
    (- 99972)))
(assert (<= (+ 0 (* 83 x1) (* 78 x2) (* (- 94) x3)) 25850))
(assert (<= (+ 0 (* (- 92) x1) (* (- 1) x2) (* (- 30) x3)) 4286))
(assert (<= (+ 0 (* 80 x1) (* (- 54) x2) (* 17 x3)) (- 15762)))
(check-sat)
(get-model)
