(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 10) (pow x1)) (* (- 28007) x1) (* (- 26802) x2) (* 9441 x3))
    (- 58233)))
(assert (<= (+ 0 (* 7 (pow x1)) (* (- 48179) x1) (* (- 74368) x2) (* 53495 x3))
    (- 8072)))
(assert (<= (+ 0 (* 22 x1) (* (- 71) x2) (* 69 x3)) 37628))
(assert (<= (+ 0 (* (- 64) x1) (* 68 x2) (* 21 x3)) (- 37273)))
(assert (<= (+ 0 (* (- 45) x1) (* 69 x2) (* 70 x3)) (- 29334)))
(check-sat)
(get-model)
