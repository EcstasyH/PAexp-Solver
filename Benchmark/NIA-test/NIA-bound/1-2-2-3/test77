(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* 10 (pow x1)) (* (- 9711) x1) (* 41008 x2) (* 85753 x3)) (- 87964)))
(assert (<= (+ 0 (* 4 (pow x1)) (* (- 13360) x1) (* 75048 x2) (* 79661 x3)) 86727))
(assert (<= (+ 0 (* 72 x1) (* 96 x2) (* 15 x3)) 14404))
(assert (<= (+ 0 (* (- 72) x1) (* (- 89) x2) (* (- 69) x3)) 9514))
(assert (<= (+ 0 (* 0 x1) (* (- 32) x2) (* 35 x3)) 92869))
(check-sat)
(get-model)
