(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 4) (pow x1)) (* 8564 x1) (* 42613 x2) (* (- 24008) x3)) 31351))
(assert (<= (+ 0 (* 6 (pow x1)) (* (- 94097) x1) (* 39388 x2) (* (- 16772) x3))
    (- 50668)))
(assert (<= (+ 0 (* (- 39) x1) (* 36 x2) (* (- 87) x3)) (- 32698)))
(assert (<= (+ 0 (* (- 9) x1) (* (- 34) x2) (* (- 46) x3)) (- 16755)))
(assert (<= (+ 0 (* (- 83) x1) (* 34 x2) (* (- 54) x3)) 80589))
(check-sat)
(get-model)
