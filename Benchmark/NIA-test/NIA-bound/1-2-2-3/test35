    (- 70661)))
(assert (<= (+ 0 (* 60 x1) (* (- 40) x2) (* 82 x3)) 7447))
(assert (<= (+ 0 (* 93 x1) (* 37 x2) (* (- 92) x3)) (- 68792)))
