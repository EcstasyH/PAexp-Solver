(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 9) (pow x1)) (* 30564 x1) (* 43252 x2) (* (- 60092) x3))
    (- 6002)))
(assert (<= (+ 0 (* (- 1) (pow x1)) (* 18244 x1) (* (- 69148) x2) (* (- 91399) x3))
    (- 78254)))
(assert (<= (+ 0 (* 35 x1) (* 28 x2) (* 54 x3)) (- 89754)))
(assert (<= (+ 0 (* 69 x1) (* (- 91) x2) (* 55 x3)) 62359))
(assert (<= (+ 0 (* 59 x1) (* 65 x2) (* 44 x3)) (- 37494)))
(check-sat)
