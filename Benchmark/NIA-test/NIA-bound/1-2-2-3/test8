(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 10) (pow x1)) (* 30090 x1) (* 17768 x2) (* 39069 x3)) (- 49440)))
(assert (<= (+ 0 (* (- 6) (pow x1)) (* (- 31025) x1) (* 18015 x2) (* 16815 x3))
    (- 76140)))
(assert (<= (+ 0 (* (- 93) x1) (* 28 x2) (* 14 x3)) (- 79680)))
(assert (<= (+ 0 (* (- 68) x1) (* (- 62) x2) (* (- 31) x3)) (- 75437)))
(assert (<= (+ 0 (* (- 29) x1) (* (- 24) x2) (* (- 76) x3)) 15546))
(check-sat)
