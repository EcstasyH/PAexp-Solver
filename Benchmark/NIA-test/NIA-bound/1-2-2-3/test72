(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 5) (pow x1)) (* 58496 x1) (* (- 5764) x2) (* 71853 x3))
    (- 16898)))
(assert (<= (+ 0 (* 1 (pow x1)) (* (- 22747) x1) (* (- 61223) x2) (* (- 75392) x3))
    (- 61868)))
(assert (<= (+ 0 (* (- 6) x1) (* 27 x2) (* 66 x3)) (- 24005)))
(assert (<= (+ 0 (* 85 x1) (* (- 10) x2) (* (- 24) x3)) (- 36033)))
(assert (<= (+ 0 (* 28 x1) (* 98 x2) (* 70 x3)) 61523))
(check-sat)
(get-model)
