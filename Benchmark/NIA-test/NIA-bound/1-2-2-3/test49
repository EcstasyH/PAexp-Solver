(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* 4 (pow x1)) (* (- 3173) x1) (* (- 1059) x2) (* (- 93835) x3)) 32520))
(assert (<= (+ 0 (* (- 9) (pow x1)) (* (- 86235) x1) (* 61808 x2) (* 68734 x3))
    (- 69151)))
(assert (<= (+ 0 (* 52 x1) (* (- 70) x2) (* 18 x3)) (- 73059)))
(assert (<= (+ 0 (* 46 x1) (* (- 75) x2) (* 82 x3)) 14170))
(assert (<= (+ 0 (* (- 13) x1) (* 51 x2) (* (- 12) x3)) 67107))
(check-sat)
(get-model)
