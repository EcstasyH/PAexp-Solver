(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 4) (pow x1)) (* (- 54634) x1) (* 41090 x2) (* 13493 x3))
    (- 82865)))
(assert (<= (+ 0 (* 6 (pow x1)) (* (- 31486) x1) (* (- 91520) x2) (* (- 94378) x3))
    (- 92744)))
(assert (<= (+ 0 (* (- 91) x1) (* (- 82) x2) (* 98 x3)) 8209))
(assert (<= (+ 0 (* 41 x1) (* (- 62) x2) (* 80 x3)) (- 72767)))
(assert (<= (+ 0 (* 75 x1) (* (- 82) x2) (* 56 x3)) (- 72279)))
(check-sat)
