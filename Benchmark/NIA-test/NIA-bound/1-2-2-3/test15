(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 5) (pow x1)) (* 26136 x1) (* (- 36573) x2) (* 84664 x3)) 86306))
(assert (<= (+ 0 (* 10 (pow x1)) (* 91029 x1) (* 50974 x2) (* 6028 x3)) 62404))
(assert (<= (+ 0 (* (- 62) x1) (* (- 14) x2) (* 44 x3)) (- 74296)))
(assert (<= (+ 0 (* 70 x1) (* 65 x2) (* (- 7) x3)) (- 70634)))
(assert (<= (+ 0 (* (- 50) x1) (* (- 8) x2) (* 42 x3)) 36601))
(check-sat)
(get-model)
