    36007))
(assert (<= (+ 0 (* 9 (pow x1)) (* 10545 x1) (* (- 76822) x2) (* (- 94704) x3)) 13760))
(assert (<= (+ 0 (* (- 14) x1) (* (- 14) x2) (* 72 x3)) 11084))
(assert (<= (+ 0 (* 74 x1) (* (- 22) x2) (* 96 x3)) 79561))
