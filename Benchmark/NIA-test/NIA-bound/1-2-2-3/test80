(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (<= (+ 0 (* (- 1) (pow x1)) (* (- 48819) x1) (* (- 54705) x2) (* (- 75139) x3))
    (- 38370)))
(assert (<= (+ 0 (* 6 (pow x1)) (* 38983 x1) (* 21485 x2) (* (- 79123) x3)) (- 85942)))
(assert (<= (+ 0 (* 35 x1) (* (- 57) x2) (* 64 x3)) 24628))
(assert (<= (+ 0 (* (- 68) x1) (* (- 19) x2) (* (- 51) x3)) (- 95016)))
(assert (<= (+ 0 (* 52 x1) (* 8 x2) (* 10 x3)) 57090))
(check-sat)
