    (- 36210)))
(assert (<= (+ 0 (* (- 1) (pow x1)) (* (- 2525) x1) (* 76765 x2) (* 12133 x3)) 97287))
(assert (<= (+ 0 (* 22 x1) (* 52 x2) (* (- 1) x3)) 9897))
(assert (<= (+ 0 (* (- 96) x1) (* 31 x2) (* 26 x3)) (- 4769)))
