    14994))
(assert (<= (+ 0 (* (- 83) x1) (* (- 11) x2) (* (- 30) x3)) (- 67608)))
(assert (<= (+ 0 (* 4 x1) (* (- 20) x2) (* (- 91) x3)) (- 89496)))
(assert (<= (+ 0 (* 94 x1) (* 76 x2) (* (- 83) x3)) 39941))
