(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(declare-fun x4 () Int)
(declare-fun x5 () Int)
(declare-fun x6 () Int)
(declare-fun x7 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (>= x4 0))
(assert (>= x5 0))
(assert (>= x6 0))
(assert (>= x7 0))
(assert (= (+ 0
      (* (- 1) (pow x1))
      (* 1 (pow x2))
      (* (- 1) (pow x3))
      (* 0 x1)
      (* 1 x2)
      (* (- 1) x3))
   0))
(assert (<= (+ 0
       (* 5 (pow x1))
       (* (- 8) (pow x2))
       (* (- 2) (pow x3))
       (* 36 x1)
       (* (- 69) x2)
       (* 44 x3))
    (- 37373)))
(assert (<= (+ 0
       (* 4 (pow x1))
       (* 8 (pow x2))
       (* (- 5) (pow x3))
       (* (- 27) x1)
       (* (- 28) x2)
       (* 36 x3))
    (- 57137)))
(assert (<= (+ 0
       (* (- 92) x1)
       (* (- 83) x2)
       (* 55 x3)
       (* (- 87) x4)
       (* (- 76) x5)
       (* 77 x6)
       (* (- 12) x7))
    (- 16900)))
(assert (<= (+ 0
       (* (- 31) x1)
       (* (- 85) x2)
       (* 93 x3)
       (* 34 x4)
       (* (- 68) x5)
       (* (- 59) x6)
       (* 69 x7))
    (- 20035)))
(assert (<= (+ 0
       (* (- 2) x1)
       (* (- 48) x2)
       (* 22 x3)
       (* 54 x4)
       (* 29 x5)
       (* 35 x6)
       (* 41 x7))
    (- 18627)))
(assert (<= (+ 0
       (* (- 10) x1)
       (* 2 x2)
       (* 83 x3)
       (* 12 x4)
       (* 1 x5)
       (* 95 x6)
       (* 25 x7))
    (- 32112)))
(check-sat)
