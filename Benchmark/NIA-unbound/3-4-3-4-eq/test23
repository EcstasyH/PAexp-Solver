(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(declare-fun x4 () Int)
(declare-fun x5 () Int)
(declare-fun x6 () Int)
(declare-fun x7 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (>= x4 0))
(assert (>= x5 0))
(assert (>= x6 0))
(assert (>= x7 0))
(assert (= (+ 0
      (* (- 1) (pow x1))
      (* 0 (pow x2))
      (* 1 (pow x3))
      (* (- 10) x1)
      (* 5 x2)
      (* (- 3) x3))
   0))
(assert (<= (+ 0
       (* 10 (pow x1))
       (* (- 10) (pow x2))
       (* (- 5) (pow x3))
       (* (- 38) x1)
       (* 46 x2)
       (* (- 60) x3))
    (- 51985)))
(assert (<= (+ 0
       (* 9 (pow x1))
       (* (- 2) (pow x2))
       (* 2 (pow x3))
       (* 27 x1)
       (* 64 x2)
       (* (- 25) x3))
    16497))
(assert (<= (+ 0
       (* (- 53) x1)
       (* 91 x2)
       (* (- 1) x3)
       (* (- 10) x4)
       (* (- 22) x5)
       (* (- 7) x6)
       (* 57 x7))
    80490))
(assert (<= (+ 0
       (* 70 x1)
       (* (- 41) x2)
       (* (- 26) x3)
       (* 17 x4)
       (* 63 x5)
       (* 99 x6)
       (* (- 34) x7))
    (- 27852)))
(assert (<= (+ 0
       (* 89 x1)
       (* (- 18) x2)
       (* 13 x3)
       (* (- 62) x4)
       (* 95 x5)
       (* (- 49) x6)
       (* 25 x7))
    86353))
(assert (<= (+ 0
       (* (- 59) x1)
       (* 46 x2)
       (* (- 84) x3)
       (* 32 x4)
       (* 76 x5)
       (* (- 7) x6)
       (* 38 x7))
    (- 95806)))
