(assert (>= x2 0))
(assert (>= x3 0))
(assert (>= x4 0))
(assert (>= x5 0))
(assert (>= x6 0))
(assert (>= x7 0))
(assert (= (+ 0
      (* (- 1) (pow x1))
      (* 0 (pow x2))
      (* (- 1) (pow x3))
      (* 0 x1)
      (* 8 x2)
      (* 0 x3))
   0))
(assert (<= (+ 0
       (* 1 (pow x1))
       (* (- 7) (pow x2))
       (* (- 3) (pow x3))
       (* 49 x1)
       (* (- 49) x2)
       (* 60 x3))
    (- 39140)))
