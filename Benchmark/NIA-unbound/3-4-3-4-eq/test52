       (* (- 5) (pow x1))
       (* (- 8) (pow x2))
       (* 1 (pow x3))
       (* (- 21) x1)
       (* 79 x2)
       (* (- 64) x3))
    (- 89417)))
(assert (<= (+ 0
       (* (- 22) x1)
       (* 80 x2)
       (* 85 x3)
       (* 45 x4)
       (* (- 43) x5)
       (* 59 x6)
       (* (- 61) x7))
    (- 22876)))
(assert (<= (+ 0
