(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(declare-fun x4 () Int)
(declare-fun x5 () Int)
(declare-fun x6 () Int)
(declare-fun x7 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (>= x4 0))
(assert (>= x5 0))
(assert (>= x6 0))
(assert (>= x7 0))
(assert (= (+ 0
      (* (- 1) (pow x1))
      (* (- 1) (pow x2))
      (* (- 1) (pow x3))
      (* 10 x1)
      (* 4 x2)
      (* 2 x3))
   0))
(assert (<= (+ 0
       (* (- 4) (pow x1))
       (* 5 (pow x2))
       (* (- 3) (pow x3))
       (* (- 86) x1)
       (* (- 12) x2)
       (* (- 28) x3))
    21073))
(assert (<= (+ 0
       (* (- 7) (pow x1))
       (* 1 (pow x2))
       (* 1 (pow x3))
       (* (- 17) x1)
       (* (- 67) x2)
       (* 83 x3))
    (- 37993)))
(assert (<= (+ 0
       (* 61 x1)
       (* (- 80) x2)
       (* (- 72) x3)
       (* 62 x4)
       (* 33 x5)
       (* 48 x6)
       (* (- 12) x7))
    91790))
(assert (<= (+ 0
       (* 39 x1)
       (* (- 75) x2)
       (* 86 x3)
       (* 40 x4)
       (* (- 77) x5)
       (* 26 x6)
       (* (- 75) x7))
    25597))
(assert (<= (+ 0
       (* (- 70) x1)
       (* (- 65) x2)
       (* 64 x3)
       (* (- 45) x4)
       (* (- 93) x5)
       (* (- 59) x6)
       (* 41 x7))
    (- 14336)))
(assert (<= (+ 0
       (* 70 x1)
       (* 36 x2)
       (* 79 x3)
       (* 45 x4)
       (* (- 45) x5)
       (* 40 x6)
