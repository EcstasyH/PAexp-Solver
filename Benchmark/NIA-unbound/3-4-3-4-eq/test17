(set-logic ALL)
(define-fun-rec pow ((x Int)) Int (ite (= x 0) 1 (* 10 (pow (- x 1)) ) ) )
(declare-fun x1 () Int)
(declare-fun x2 () Int)
(declare-fun x3 () Int)
(declare-fun x4 () Int)
(declare-fun x5 () Int)
(declare-fun x6 () Int)
(declare-fun x7 () Int)
(assert (>= x1 0))
(assert (>= x2 0))
(assert (>= x3 0))
(assert (>= x4 0))
(assert (>= x5 0))
(assert (>= x6 0))
(assert (>= x7 0))
(assert (= (+ 0
      (* 1 (pow x1))
      (* (- 1) (pow x2))
      (* (- 1) (pow x3))
      (* 10 x1)
      (* 3 x2)
      (* 1 x3))
   0))
(assert (<= (+ 0
       (* 6 (pow x1))
       (* (- 8) (pow x2))
       (* 4 (pow x3))
       (* 64 x1)
       (* 35 x2)
       (* (- 6) x3))
    (- 10161)))
(assert (<= (+ 0
       (* (- 3) (pow x1))
       (* (- 2) (pow x2))
       (* 9 (pow x3))
       (* 49 x1)
       (* (- 66) x2)
       (* 18 x3))
    (- 73771)))
(assert (<= (+ 0
       (* 68 x1)
       (* 84 x2)
       (* 64 x3)
       (* 89 x4)
       (* (- 93) x5)
       (* 46 x6)
       (* 16 x7))
    (- 97037)))
(assert (<= (+ 0
       (* 3 x1)
       (* (- 11) x2)
       (* 77 x3)
       (* (- 35) x4)
       (* 26 x5)
       (* 86 x6)
       (* (- 86) x7))
    36317))
(assert (<= (+ 0
       (* (- 98) x1)
       (* (- 64) x2)
       (* (- 94) x3)
       (* 61 x4)
       (* (- 65) x5)
       (* 85 x6)
       (* 1 x7))
    (- 77113)))
(assert (<= (+ 0
       (* (- 85) x1)
       (* (- 71) x2)
       (* 37 x3)
       (* 8 x4)
