(assert (>= x6 0))
(assert (>= x7 0))
(assert (= (+ 0
      (* (- 1) (pow x1))
      (* (- 1) (pow x2))
      (* (- 1) (pow x3))
      (* (- 1) x1)
      (* (- 7) x2)
      (* 7 x3))
   0))
